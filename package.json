    "@react-pdf-viewer/core": "^3.10.0",
    "axios": "^1.2.1",
    "bcrypt": "^5.1.0",
    "chart.js": "^4.2.0",
    "cors": "^2.8.5",
    "js-sha256": "^0.9.0",
    "leaflet": "^1.9.3",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.9.0",
    "react": "^18.2.0",
    "react-calendar": "^4.0.0",
    "react-chartjs-2": "^5.2.0",
