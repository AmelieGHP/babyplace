    "react-modal": "^3.16.1",
    "react-rating": "^2.0.5",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.4.4",
    "react-router-hash-link": "^2.4.3",
    "react-scroll-to-bottom": "^4.2.0",
    "sass": "^1.56.1",
    "socket-io": "^1.0.0",
